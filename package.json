{
  "name": "contentful-to-sanity",
  "version": "4.2.7",
  "description": "Migration tool from Contentful to Sanity",
  "keywords": [
    "sanity",
    "contentful",
    "migration"
  ],
  "homepage": "https://github.com/sanity-io/contentful-to-sanity",
  "bugs": {
    "url": "https://github.com/sanity-io/contentful-to-sanity/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/contentful-to-sanity.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "source": "./src/cli/index.ts",
      "require": "./dist/cli.cjs",
      "default": "./dist/cli.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "cli": [
        "./dist/cli.d.ts"
      ]
    }
  },
  "bin": {
    "contentful-to-sanity": "./bin/contentful-to-sanity.cjs"
  },
  "files": [
    "bin",
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "contentful-to-sanity": "./bin/contentful-to-sanity.cjs",
    "format": "prettier --cache --ignore-path .gitignore --write .",
    "lint": "TIMING=1 eslint . --cache",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "rm -rf dist && pkg build --strict",
    "test": "npx cross-env TZ=UTC vitest",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    "> 0.2% and supports es6-module and supports es6-module-dynamic-import and not dead and not IE 11",
    "maintained node versions"
  ],
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@portabletext/contentful-rich-text-to-portable-text": "0.1.0",
    "@sanity/block-tools": "3.17.0",
    "@sanity/client": "6.4.12",
    "@sanity/schema": "3.17.0",
    "@stdlib/assert-is-absolute-path": "0.1.1",
    "@stdlib/assert-is-relative-path": "0.1.1",
    "case": "1.6.3",
    "commander": "11.0.0",
    "contentful-export": "7.19.83",
    "contentful-management": "11.1.1",
    "javascript-stringify": "2.1.0",
    "jsdom": "22.1.0",
    "just-compact": "3.2.0",
    "just-omit": "2.2.0",
    "just-safe-get": "4.2.0",
    "just-safe-set": "4.2.1",
    "markdown-it": "13.0.2",
    "mkdirp": "3.0.1",
    "node-fetch": "^2.6.7",
    "object-hash": "^3.0.0",
    "prettier": "3.0.3",
    "sharp": "^0.32.0",
    "svgo": "^3.0.2",
    "tiny-invariant": "1.3.1",
    "valid-filename": "4.0.0",
    "zod": "3.22.4",
    "zod-validation-error": "3.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@sanity/pkg-utils": "^4.4.2",
    "@sanity/semantic-release-preset": "^4.1.4",
    "@sanity/types": "^3.17.0",
    "@types/fs-extra": "^11.0.2",
    "@types/jsdom": "^21.1.3",
    "@types/markdown-it": "^13.0.2",
    "@types/node": "^18.18.4",
    "@types/node-fetch": "^2.6.6",
    "@types/object-hash": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitest/coverage-v8": "^0.34.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "ls-engines": "^0.9.0",
    "moment": "^2.29.4",
    "prettier-plugin-packagejson": "^2.4.6",
    "rimraf": "^5.0.0",
    "sanity": "^3.17.0",
    "typescript": "5.4.2",
    "vitest": "^0.34.6",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
