          {
            "_key": "bb463b8b",
            "_type": "span",
            "marks": [
              "a27218b8",
            ],
            "text": "Intermediate",
          },
          {
            "_key": "bb463b8b",
            "_type": "span",
            "marks": [],
            "text": ", or ",
          },
          {
            "_key": "bb463b8b",
            "_type": "span",
            "marks": [
              "a27218b8",
            ],
            "text": "Advanced",
          },
          {
