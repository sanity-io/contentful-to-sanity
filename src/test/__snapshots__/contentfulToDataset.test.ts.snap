  {
    "_createdAt": "2022-06-16T13:45:54.978Z",
    "_id": "iTL483Aq7Z4khJHcalSrh",
    "_rev": "iTL483Aq7Z4khJHcalSrh",
    "_type": "kbAppArticle",
    "_updatedAt": "2022-06-20T16:10:30.080Z",
    "body": [
      {
        "_key": "bb463b8b",
        "_type": "block",
        "children": [
          {
            "_key": "bb463b8b",
            "_type": "span",
            "marks": [],
            "text": "To embed a code block into the article you have to do the following steps:",
          },
        ],
        "markDefs": [],
        "style": "normal",
      },
      {
        "_key": "bb463b8b",
        "_type": "block",
        "children": [
          {
            "_key": "bb463b8b",
            "_type": "span",
            "marks": [],
            "text": "Navigate to your Contentful space content tab.",
          },
        ],
        "level": 1,
        "listItem": "number",
        "markDefs": [],
        "style": "normal",
      },
      {
        "_key": "bb463b8b",
        "_type": "block",
        "children": [
          {
            "_key": "bb463b8b",
            "_type": "span",
            "marks": [],
            "text": "Navigate to the article you want to embed a code block in.",
          },
        ],
        "level": 1,
        "listItem": "number",
        "markDefs": [],
        "style": "normal",
      },
      {
        "_key": "bb463b8b",
        "_type": "block",
        "children": [
          {
            "_key": "bb463b8b",
            "_type": "span",
            "marks": [],
            "text": "Find a RichText editor with your article body content.",
          },
        ],
        "level": 1,
        "listItem": "number",
        "markDefs": [],
        "style": "normal",
      },
      {
